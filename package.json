{
  "name": "brainjs",
  "description": "increase conversions with a single line of code",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist/*",
    "test-client": "export NODE_ENV=test && ./node_modules/.bin/karma start",
    "test-server": "export NODE_ENV=test DATASTORE_EMULATOR_HOST=localhost:8173 GCLOUD_PROJECT=nth-name-156816 GOOGLE_APPLICATION_CREDENTIALS=/Users/ohad/IdeaProjects/brainjs/google.credentials.json && ./node_modules/.bin/mocha ./src/server/**/*.test.js",
    "test-common": "export NODE_ENV=test && ./node_modules/.bin/mocha ./src/common/**/*.test.js",
    "bundle": "webpack --config ./webpack.config.js --progress --colors",
    "build": "export NODE_ENV=development && npm run bundle",
    "production": "export NODE_ENV=production && npm run clean && npm run bundle",
    "staging": "export NODE_ENV=staging && npm run clean && npm run bundle",
    "app-prod": "gcloud app deploy --project=nth-name-156816 ./app.yaml",
    "app-staging": "gcloud app deploy --project=brainjs-staging ./staging.yaml",
    "watch": "export NODE_ENV=development && webpack --config ./webpack.config.js --watch",
    "commit": "commit-wizard",
    "start": "node ./src/server/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mybrainpal/brainjs.git"
  },
  "keywords": [
    "awesomeness",
    "tarefet"
  ],
  "engines": {
    "node": ">=7.5.0",
    "npm": ">=4.1.2"
  },
  "author": "BrainPal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mybrainpal/brainjs/issues"
  },
  "homepage": "https://github.com/mybrainpal/brainjs#readme",
  "dependencies": {
    "@google-cloud/datastore": "^0.7.0",
    "@google-cloud/storage": "^0.7.0",
    "alertifyjs": "^1.9.0",
    "body-parser": "^1.16.1",
    "express": "^4.14.1",
    "flat": "^2.0.1",
    "lodash": "^4.17.4",
    "stacktrace-js": "^1.3.1",
    "sweetalert2": "^6.4.2",
    "tinysort": "^2.3.6",
    "typer-js": "^2.4.3"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "bluebird": "^3.4.7",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-http": "^3.0.0",
    "chai-spies": "^0.7.1",
    "chromedriver": "^2.27.3",
    "css-loader": "^0.26.2",
    "file-loader": "^0.10.1",
    "fs": "0.0.1-security",
    "fs-extra": "^2.0.0",
    "glob": "^7.1.1",
    "inject-loader": "^3.0.0-beta4",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-webpack": "^2.0.2",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "nightwatch": "^0.9.12",
    "node-sass": "^4.5.0",
    "path": "^0.12.7",
    "pre-git": "^3.14.0",
    "prop-types": "^0.2.0",
    "recursive-readdir": "^2.1.1",
    "sass-loader": "^6.0.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1"
  },
  "config": {
    "pre-git": {
      "enabled": true,
      "pre-push": [
        "npm run test-server",
        "npm run test-client",
        "npm run test-common"
      ],
      "post-checkout": []
    }
  }
}
