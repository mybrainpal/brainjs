{
  "name": "brainjs",
  "description": "increase conversions with a single line of code",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist/*",
    "test-client": "./node_modules/.bin/karma start",
    "test-server": "./node_modules/.bin/mocha ./src/server/**/*.test.js",
    "test-common": "./node_modules/.bin/mocha ./src/common/**/*.test.js",
    "pre-test": "export NODE_ENV=test",
    "bundle": "webpack --config ./webpack.config.js --progress --colors",
    "build": "export NODE_ENV=development && npm run bundle",
    "production": "export NODE_ENV=production && npm run clean && npm run bundle",
    "staging": "export NODE_ENV=staging && npm run clean && npm run bundle",
    "watch": "export NODE_ENV=development && webpack --config ./webpack.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mybrainpal/brainjs.git"
  },
  "keywords": [
    "cro",
    "optimization"
  ],
  "engines": {
    "node": ">=7.5.0",
    "npm": ">=4.1.2"
  },
  "author": "BrainPal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mybrainpal/brainjs/issues"
  },
  "homepage": "https://github.com/mybrainpal/brainjs#readme",
  "pre-push": {
    "silent": false,
    "run": [
      "pre-test",
      "test-client",
      "test-server",
      "test-common"
    ]
  },
  "dependencies": {
    "@google-cloud/storage": "^0.7.0",
    "alertifyjs": "^1.9.0",
    "stacktrace-js": "^1.3.1",
    "sweetalert2": "^6.3.8",
    "tinysort": "^2.3.6",
    "typer-js": "^2.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "bluebird": "^3.4.7",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-http": "^3.0.0",
    "chai-spies": "^0.7.1",
    "chromedriver": "^2.27.2",
    "css-loader": "^0.26.1",
    "express": "^4.14.1",
    "file-loader": "^0.10.0",
    "fs": "0.0.1-security",
    "fs-extra": "^2.0.0",
    "glob": "^7.1.1",
    "inject-loader": "^3.0.0-beta4",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-webpack": "^2.0.2",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "nightwatch": "^0.9.12",
    "node-sass": "^4.5.0",
    "path": "^0.12.7",
    "pre-push": "^0.1.1",
    "prop-types": "^0.2.0",
    "recursive-readdir": "^2.1.0",
    "sass-loader": "^5.0.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1"
  }
}
